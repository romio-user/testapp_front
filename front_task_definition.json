{
    "family": "front",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "environment": [],
            "name": "front",
            "image": "${REPOSITORY_URI}:${IMAGE_TAG}",
            "cpu": 0,
            "portMappings": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-region": "${env.AWS_DEFAULT_REGION}",
                    "awslogs-stream-prefix": "ecs",
                    "awslogs-group": "/ecs/testapp"
                    }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "${env.EXECUTION_ROLE_ARN}"
}
